<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:layout_marginBottom="60dp"

    >
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/main_lay">
        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:src="@drawable/like_off"
            android:id="@+id/like"
            android:padding="12dp"
            android:layout_centerVertical="true"/>
        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:src="@drawable/dislike_off"
            android:id="@+id/dislike"
            android:layout_centerVertical="true"
            android:padding="12dp"
            android:layout_toRightOf="@+id/like"/>
        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:src="@drawable/scribble_vies"
            android:id="@+id/paint"
            android:padding="12dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/dislike"/>
        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:src="@drawable/share_slide"
            android:layout_centerVertical="true"
            android:id="@+id/share"
            android:padding="12dp"
            android:layout_toRightOf="@+id/paint"/>
        <ImageView
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_centerVertical="true"
            android:src="@drawable/slide_feed"
            android:id="@+id/feedback"
            android:padding="12dp"
            android:layout_toRightOf="@+id/share"/>
    </RelativeLayout>

</RelativeLayout>